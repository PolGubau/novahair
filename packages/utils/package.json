{
	"name": "@novahair/utils",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./src/index.ts"
		},
		"./lib/*": {
			"types": "./src/lib/*.ts",
			"default": "./src/lib/*.ts"
		},
		"./hooks/*": {
			"types": "./src/hooks/*.ts",
			"default": "./src/hooks/*.ts"
		},
		"./types/*": {
			"types": "./src/types/*.ts",
			"default": "./src/types/*.ts"
		},
		"./i18n/*": {
			"types": "./src/i18n/*.ts",
			"default": "./src/i18n/*.ts"
		},
		"./constants": {
			"types": "./src/constants.ts",
			"default": "./src/constants.ts"
		}
	},
	"scripts": {
		"lint": "biome lint",
		"format": "biome format",
		"check": "biome check --write",
		"test": "vitest run"
	},
	"peerDependencies": {
		"clsx": "^2.0.0",
		"date-fns": "^4.0.0",
		"i18next": "^25.0.0",
		"react": "^19.0.0",
		"react-i18next": "^15.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/react": "^19.0.8",
		"typescript": "^5.7.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.90.19"
	}
}
